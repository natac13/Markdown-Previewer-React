{
  "name": "markdown-previewer-react",
  "version": "0.1.0",
  "description": "Markdown previewer built in React",
  "main": "server.js",
  "scripts": {
    "start": "babel-node server.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ignore-styles --require ./test/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf ./build/",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack.production.config.js --progress --color",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "commit": "git-cz",
    "postinstall": "webpack -p --config ./webpack.production.config.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natac13/Markdown-Previewer-React.git"
  },
  "keywords": [
    "markdown",
    "react"
  ],
  "author": "Sean Campbell <seancampbell13@gmail.com> (http://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/Markdown-Previewer-React/issues"
  },
  "homepage": "https://github.com/natac13/Markdown-Previewer-React#readme",
  "dependencies": {
    "babel": "6.3.26",
    "babel-cli": "6.3.17",
    "classnames": "2.2.1",
    "marked": "0.3.5",
    "react": "0.14.5",
    "react-dom": "0.14.5"
  },
  "devDependencies": {
    "autoprefixer": "6.2.3",
    "babel-core": "6.3.26",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "babel-preset-stage-0": "6.3.13",
    "chai": "3.4.1",
    "classnames": "2.2.1",
    "commitizen": "2.4.6",
    "cross-env": "1.0.6",
    "css-loader": "0.23.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-react": "3.13.1",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "0.9.1",
    "gh-pages": "0.8.0",
    "html-webpack-plugin": "1.7.0",
    "ignore-styles": "1.2.0",
    "jsdom": "7.2.2",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "normalize.css": "3.0.3",
    "path": "0.12.7",
    "postcss-loader": "0.8.0",
    "rimraf": "2.5.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
